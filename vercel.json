{
  "buildCommand": null,
  "outputDirectory": "./",
  "installCommand": null,
  "devCommand": null,
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "^/$",
      "dest": "/slides/index.html"
    },
    {
      "src": "^/index.html$",
      "dest": "/slides/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}